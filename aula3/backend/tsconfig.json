{
  "compilerOptions": {
    "module": "commonjs",  // Use "commonjs" para compatibilidade com Jest
    "target": "ES6",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist"
  },
  "include": ["src/**/*.ts", "tests/**/*.ts"]
}